tton class=\"button\" v-on:click=\"create\">\n      Create\n    </button>\n    <div style=\"display: flex; justify-content: center; align-items: center;margin-top: 60px;\">\n      <van-cell-group inset>\n        <van-field v-model=\"room\" label=\"Room\" type=\"digital\" placeholder=\"Please Enter Room\" style=\"font-size:40px;\" />\n      </van-cell-group>\n    </div>\n    <van-button @click=\"enter\" round block style=\"font-size: 18px; height: 32px;\">\n      Enter\n    </van-button>\n  </div>\n  <div v-show=\"!isOnline\">\n    <button class=\"button\" v-on:click=\"local\">\n      Local\n    </button>\n    <button class=\"button\" v-on:click=\"online\">\n      Online\n    </button>\n    <button class=\"button\" v-on:click=\"computer\">\n      Computer\n    </button>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\"\nimport Cookies from 'js-cookie'\nexport default {\n  name: \"five-to-a-row\",\n  data() {\n    return {\n      room: '',\n      isOnline: false,\n\n    }\n  },\n  created() {\n    Cookies.remove('room')\n  },\n  methods: {\n    local() {\n      this.$router.push('/local');\n    },\n    online() {\n      this.isOnline = true\n    },\n    computer() {\n      this.$router.push('/local');\n    },\n    create() {\n      axios.post(\"http://localhost:1919/chess/create\", {\n      }).then(res => {\n        if (res.status == 200) {\n          let rooms = res.data.rooms\n          Cookies.set('room', rooms)\n          this.$router.push({ name: 'online', params: { room: rooms } });\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n    enter() {\n      axios.post(\"http://localhost:1919/chess/enter\", {\n        rooms: this.room,\n      }).then(res => {\n        if (res.status == 200) {\n          if (res.data != '') {\n            let rooms = res.data.rooms\n            Cookies.set('room', rooms)\n            this.$router.push({ name: 'online', params: { room: rooms } });\n          }else{\n            window.alert(\"No Such Room!\");\n          }\n        }\n      }).catch(err => {\n        console.log(err)\n      })\n    },\n    back() {\n      this.isOnline = false\n    }\n  },\n}\n</script>\n \n<style>\n.button {\n  display: block;\n  margin: auto;\n  margin-top: 60px;\n  font-size: 32px;\n}\n\n.title {\n  text-align: center;\n  font-size: 32px;\n  font-weight: 800;\n}\n</style>"]}]}