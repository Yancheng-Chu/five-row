++) {\r\n                    for (let k = 0; k < 5; k++) {\r\n                        this.wins[j + k][i][this.count] = true;\r\n                    }\r\n                    this.count++;\r\n                }\r\n            }\r\n        },\r\n        slash() {\r\n\r\n            for (let i = 0; i < 11; i++) {\r\n                for (let j = 0; j < 11; j++) {\r\n                    for (let k = 0; k < 5; k++) {\r\n                        this.wins[i + k][j + k][this.count] = true;\r\n                    }\r\n                    this.count++;\r\n                }\r\n            }\r\n        },\r\n        reverse() {\r\n\r\n            for (let i = 0; i < 11; i++) {\r\n                for (let j = 14; j > 3; j--) {\r\n                    for (let k = 0; k < 5; k++) {\r\n                        this.wins[i + k][j - k][this.count] = true;\r\n                    }\r\n                    this.count++;\r\n                }\r\n            }\r\n        },\r\n        init() {\r\n            for (let i = 0; i < this.count; i++) {\r\n                this.blackWin[i] = 0;\r\n                this.whiteWin[i] = 0;\r\n                this.computerWin[i] = 0;\r\n            }\r\n        },\r\n        drawChessBoard() {\r\n            for (let i = 0; i < 15; i++) {\r\n                this.ctx.moveTo(20 + i * 40, 20);\r\n                this.ctx.lineTo(20 + i * 40, 580);\r\n                this.ctx.stroke();\r\n                this.ctx.moveTo(20, 20 + i * 40);\r\n                this.ctx.lineTo(580, 20 + i * 40);\r\n                this.ctx.stroke();\r\n            }\r\n        },\r\n        drawChess(x, y) {\r\n            this.ctx.beginPath();\r\n            this.ctx.arc(20 + x * 40, 20 + y * 40, 15, 0, 2 * Math.PI);\r\n            this.ctx.closePath();\r\n            let grd = this.ctx.createRadialGradient(\r\n                25 + x * 40,\r\n                15 + y * 40,\r\n                15,\r\n                20 + x * 40,\r\n                20 + y * 40,\r\n                35\r\n  